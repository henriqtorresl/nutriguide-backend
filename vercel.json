
{
  "version": 2,
  "builds": [
    {
      "src": "./dist/server.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/swagger-ui/(.*)",
      "dest": "/node_modules/swagger-ui-dist/$1"
    },
    {
      "src": "/documentation/(.*)",
      "dest": "/src/documentation/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/server.js"
    }
  ]
}